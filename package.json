{
  "name": "stencil-render-prop",
  "version": "1.0.0",
  "description": "Example of using Stencil render prop",
  "main": "dist/stencil-render-prop.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prerender",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "copy-static": "cp -r ./static/ ./dist",
    "deploy": "gh-pages -d dist",
    "serve": "stencil-dev-server",
    "serve.prod": "npm run build && npm run copy-static && serve ./dist",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "author": "Jag Reehal",
  "license": "ISC",
  "dependencies": {
    "@stencil/core": "^0.2.2"
  },
  "devDependencies": {
    "@stencil/dev-server": "0.0.18",
    "@stencil/utils": "^0.0.4",
    "@types/jest": "^22.1.0",
    "gh-pages": "^1.1.0",
    "jest": "^22.1.4",
    "prettier": "^1.10.2",
    "serve": "^6.4.9",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
